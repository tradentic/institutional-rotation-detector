{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@libs/*": ["../../libs/*/src"]
    },
    "outDir": "dist",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["vitest/globals"],
    "baseUrl": "./",
    "paths": {
      "@libs/finra-client": ["../../libs/finra-client/src"],
      "@libs/openai-client": ["../../libs/openai-client/src"],
      "@libs/unusualwhales-client": ["../../libs/unusualwhales-client/src"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist", "src/__tests__"]
}
