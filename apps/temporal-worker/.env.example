SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=

# SEC EDGAR Configuration
# IMPORTANT: Replace with your actual name and email
# Format: "YourName your.email@domain.com"
SEC_USER_AGENT=YourCompany contact@yourcompany.com
# Or use EDGAR_USER_AGENT for backwards compatibility
EDGAR_USER_AGENT="YourAppName your-email@example.com"

# SEC Base URLs
# Main SEC site for /files/, /Archives/ endpoints
EDGAR_BASE=https://www.sec.gov
# Data API for /submissions/ endpoint
EDGAR_DATA_API_BASE=https://data.sec.gov

# Optional: Override the SEC ticker lookup endpoint
# Default: /files/company_tickers.json
# SEC_TICKER_ENDPOINT=/files/company_tickers.json

# Redis Configuration for SEC API Response Caching
# Cache SEC EDGAR API responses to avoid throttling from repeated requests
REDIS_HOST=localhost
REDIS_PORT=6379
# REDIS_PASSWORD=
# Set to 'false' to disable caching (default: enabled)
REDIS_ENABLE_CACHING=true

ISHARES_FUNDS=IWB,IWM,IWN,IWC
OPENAI_API_KEY=

# FINRA API Configuration
# OAuth2 client credentials for FINRA API access
# Get credentials from: https://www.finra.org/finra-data
FINRA_API_CLIENT=
FINRA_API_SECRET=
# Optional: Override FINRA API endpoints
# FINRA_API_BASE=https://api.finra.org
# FINRA_TOKEN_URL=https://ews.fip.finra.org/fip/rest/ews/oauth2/access_token

TEMPORAL_NAMESPACE=
TEMPORAL_TASK_QUEUE=rotation-detector
MAX_RPS_EDGAR=8
QUARTER_BATCH=8

FINRA_OTC_BASE_URL=https://api.finra.org
FINRA_API_KEY=your-finra-api-key-here
FINRA_API_CLIENT=
FINRA_API_SECRET=